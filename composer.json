{
    "name": "koomai/laravel-cli-scheduler",
    "description": "Dynamically schedule your Laravel tasks using artisan commands.",
    "keywords": [
        "koomai",
        "laravel",
        "laravel-cli-scheduler"
    ],
    "homepage": "https://github.com/koomai/laravel-cli-scheduler",
    "license": "MIT",
    "authors": [
        {
            "name": "Sid K",
            "email": "sid@koomai.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "bensampo/laravel-enum": "^3.4",
        "dragonmantank/cron-expression": "^3.1",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.1",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Koomai\\CliScheduler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Koomai\\CliScheduler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CliSchedulerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
